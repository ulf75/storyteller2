{
    "trailingComma": "none",
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 150,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": true,
    "quoteProps": "preserve",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": "*.yml",
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
